{
    "openapi": "3.0.0",
    "info": {
        "title": "Resto Backend Documentation",
        "description": "Resto Backend Documentation",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://127.0.0.1:8000/api",
            "description": "Local server"
        }
    ],
    "paths": {
        "/login": {
            "post": {
                "tags": [
                    "AuthController"
                ],
                "summary": "Login user",
                "description": "Login user dengan email dan password untuk mendapatkan API token",
                "operationId": "d618a255475240ba3d40cc39692cd924",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "user@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "password123"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login berhasil",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "example": "success"
                                        },
                                        "code": {
                                            "type": "integer",
                                            "example": 200
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Login berhasil"
                                        },
                                        "data": {
                                            "properties": {
                                                "token": {
                                                    "type": "string",
                                                    "example": "1|sometokenvalue"
                                                },
                                                "user": {
                                                    "properties": {
                                                        "id": {
                                                            "type": "string",
                                                            "example": "uuid-123"
                                                        },
                                                        "name": {
                                                            "type": "string",
                                                            "example": "John Doe"
                                                        },
                                                        "email": {
                                                            "type": "string",
                                                            "example": "user@example.com"
                                                        },
                                                        "role": {
                                                            "type": "string",
                                                            "example": "admin"
                                                        }
                                                    },
                                                    "type": "object"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Password salah"
                    },
                    "404": {
                        "description": "User tidak ditemukan"
                    },
                    "422": {
                        "description": "Validasi gagal"
                    }
                }
            }
        },
        "/profile": {
            "get": {
                "tags": [
                    "AuthController"
                ],
                "summary": "Get user profile",
                "description": "Mengambil data profile user berdasarkan token login (auth:sanctum required)",
                "operationId": "89b41a6563613a8c5f3062bc9ea139be",
                "responses": {
                    "200": {
                        "description": "Profile retrieved",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "example": "success"
                                        },
                                        "code": {
                                            "type": "integer",
                                            "example": 200
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Profile retrieved"
                                        },
                                        "data": {
                                            "properties": {
                                                "id": {
                                                    "type": "string",
                                                    "example": "uuid-123"
                                                },
                                                "name": {
                                                    "type": "string",
                                                    "example": "John Doe"
                                                },
                                                "email": {
                                                    "type": "string",
                                                    "example": "user@example.com"
                                                },
                                                "role": {
                                                    "type": "string",
                                                    "example": "admin"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthenticated"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/logout": {
            "post": {
                "tags": [
                    "AuthController"
                ],
                "summary": "Logout user",
                "description": "Menghapus token akses user sehingga tidak bisa dipakai lagi (auth:sanctum required)",
                "operationId": "1e357eb2c93a1bdabebcebfa088332bf",
                "responses": {
                    "200": {
                        "description": "Logout berhasil",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "example": "success"
                                        },
                                        "code": {
                                            "type": "integer",
                                            "example": 200
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Logout berhasil"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthenticated"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/foods": {
            "get": {
                "tags": [
                    "FoodsController"
                ],
                "summary": "Get all foods",
                "description": "Mengambil semua daftar foods yang tersedia. (hanya user dengan role pelayan)",
                "operationId": "24f4c6639c1d6acbd982b5854213dc9c",
                "responses": {
                    "200": {
                        "description": "Daftar foods berhasil diambil",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "example": "success"
                                        },
                                        "code": {
                                            "type": "integer",
                                            "example": 200
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Daftar foods"
                                        },
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "id": {
                                                        "type": "string",
                                                        "example": "uuid-123"
                                                    },
                                                    "name": {
                                                        "type": "string",
                                                        "example": "Nasi Goreng"
                                                    },
                                                    "description": {
                                                        "type": "string",
                                                        "example": "Nasi goreng dengan ayam"
                                                    },
                                                    "price": {
                                                        "type": "number",
                                                        "format": "float",
                                                        "example": 25000
                                                    },
                                                    "category": {
                                                        "type": "string",
                                                        "enum": [
                                                            "food",
                                                            "drink",
                                                            "snack"
                                                        ],
                                                        "example": "food"
                                                    },
                                                    "is_active": {
                                                        "type": "boolean",
                                                        "example": true
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "User tidak memiliki akses untuk mengambil data"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "FoodsController"
                ],
                "summary": "Add data foods",
                "description": "Menambahkan data foods baru. (hanya user dengan role pelayan)",
                "operationId": "47fb464d3147bb08b40bc2cdcb5ff021",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "price",
                                    "category"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Nama foods"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "Deskripsi foods"
                                    },
                                    "price": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 20000
                                    },
                                    "category": {
                                        "type": "string",
                                        "enum": [
                                            "food",
                                            "drink",
                                            "snack"
                                        ],
                                        "example": "food"
                                    },
                                    "is_active": {
                                        "type": "boolean",
                                        "example": true
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Data berhasil disimpan"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "User tidak memiliki akses untuk menambahkan data"
                    },
                    "409": {
                        "description": "Data foods dengan nama, harga, dan kategori yang sama sudah ada"
                    },
                    "422": {
                        "description": "Validasi gagal"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/foods/{id}": {
            "get": {
                "tags": [
                    "FoodsController"
                ],
                "summary": "Detail foods",
                "description": "Mengambil detail foods berdasarkan id. (hanya user dengan role pelayan)",
                "operationId": "31c172cf89934677906c196f74db43d4",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "UUID foods",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Detail data foods berhasil diambil"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "User tidak memiliki akses untuk menambahkan data"
                    },
                    "404": {
                        "description": "Data tidak ditemukan"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "put": {
                "tags": [
                    "FoodsController"
                ],
                "summary": "Update foods",
                "description": "Memperbarui data foods berdasarkan id. (hanya user dengan role pelayan)",
                "operationId": "5010343dcd8e7a8d103461352ab2f9b6",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "UUID foods",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Nama foods"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "Deskripsi foods"
                                    },
                                    "price": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 20000
                                    },
                                    "category": {
                                        "type": "string",
                                        "enum": [
                                            "food",
                                            "drink",
                                            "snack"
                                        ],
                                        "example": "food"
                                    },
                                    "is_active": {
                                        "type": "boolean",
                                        "example": true
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Data berhasil diperbarui"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "User tidak memiliki akses untuk menambahkan data"
                    },
                    "404": {
                        "description": "Data tidak ditemukan"
                    },
                    "422": {
                        "description": "Validasi gagal"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/foods/${id}": {
            "delete": {
                "tags": [
                    "FoodsController"
                ],
                "summary": "Delete foods",
                "description": "Hapus data foods berdasarkan id. (hanya user dengan role pelayan)",
                "operationId": "92dd39e18c15acae0c11e0cfdbfb0c8b",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "UUID foods",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Makanan berhasil dihapus"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "User tidak memiliki akses untuk menambahkan data"
                    },
                    "404": {
                        "description": "Makanan tidak ditemukan"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    },
    "tags": [
        {
            "name": "AuthController",
            "description": "AuthController"
        },
        {
            "name": "FoodsController",
            "description": "FoodsController"
        }
    ]
}